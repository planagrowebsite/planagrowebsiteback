<!DOCTYPE html>
<html lang="pt-br">

<head>
	<%- include('../includes/imports/head') %>
</head>

<body class="pagewrap wow fadeIn">
	<%- include('../includes/components/nav') %>

	<!-- Slider Part Start -->
	<section id="slider-fixed" class="img-webp">
		<div class="fix-banner">
			<div class="carousel-caption">
				<div class="container">
					<div class="slider-search-2">
						<h1>Encontre e compre Imóveis Rurais e Urbanos!</h1>
						<div class="imovel_search_form">
							<form action="/comprar" method="GET" class="imovel_filter_input">
								<input type="hidden" name="filtrando" id="inputFiltrando">
								<input type="hidden" name="selectedTipo" value="">
								<div class="row">
									<div class="col-md-2 col-sm-6">
										<select name="zona" class="selectpicker form-control" id="selectZona">
											<option disabled selected value="todos">Zona</option>
											<option value="Urbana">Urbana</option>
											<option value="Rural">Rural</option>
										</select>
									</div>
									<div class="col-md-3 col-sm-6">
										<select name="tipo" class="selectpicker form-control" id="selectTipo">
											<option disabled selected label="Zona não selecionada"> Tipo de imóvel</option>
											<optgroup label="Zona não selecionada">
												<option></option>
											</optgroup>
										</select>
									</div>
									<div class="col-md-2 col-sm-6 rural-form hidden">
										<select name="extensaomin" class="selectpicker form-control">
											<option selected disabled>Extensão Mín.</option>
											<option value="0">Qualquer valor</option>
											<option value="10">10 hec.</option>
											<option value="50">50 hec.</option>
											<option value="150">150 hec.</option>
											<option value="300">300 hec.</option>
											<option value="500">500 hec.</option>
											<option value="700">700 hec.</option>
											<option value="1000">1000 hec.</option>
										</select>
									</div>
									<div class="col-md-2 col-sm-6 rural-form hidden">
										<select name="extensaomax" class="selectpicker form-control">
											<option selected disabled>Extensão Máx.</option>
											<option value="1000000000000">Qualquer valor</option>
											<option value="10">10 hec.</option>
											<option value="50">50 hec.</option>
											<option value="150">150 hec.</option>
											<option value="300">300 hec.</option>
											<option value="500">500 hec.</option>
											<option value="700">700 hec.</option>
											<option value="1000">1000 hec.</option>
										</select>
									</div>
									<div class="col-md-2 col-sm-6 urbano-form">
										<select name="valmin" class="selectpicker form-control">
											<option selected disabled>Valor Min.</option>
											<option value="0">Qualquer valor</option>
											<option value="100000">R$100,000,00</option>
											<option value="200000">R$200,000,00</option>
											<option value="400000">R$400,000,00</option>
											<option value="600000">R$600,000,00</option>
											<option value="800000">R$800,000,00</option>
											<option value="1000000">R$1,000,000,00</option>
											<option value="1500000">R$1,500,000,00</option>
											<option value="2500000">R$2,500,000,00</option>
											<option value="3000000">R$3,000,000,00</option>
										</select>
									</div>
									<div class="col-md-2 col-sm-6 urbano-form">
										<select name="valmax" class="selectpicker form-control">
											<option selected disabled>Valor Max.</option>
											<option value="1000000000000">Qualquer valor</option>
											<option value="100000">R$100,000,00</option>
											<option value="200000">R$200,000,00</option>
											<option value="400000">R$400,000,00</option>
											<option value="600000">R$600,000,00</option>
											<option value="800000">R$800,000,00</option>
											<option value="1000000">R$1,000,000,00</option>
											<option value="1500000">R$1,500,000,00</option>
											<option value="2500000">R$2,500,000,00</option>
											<option value="3000000">R$3,000,000,00</option>
										</select>
									</div>
									<div class="col-md-3 col-sm-4">
										<input type="submit" name="search" value="Pesquisar" class="btn btn-default">
									</div>
								</div>
							</form>
						</div>
						<div class="advance-page">
							<a href="/comprar" class="btn btn-primary">Pesquisa Avançada</a>
						</div>
					</div>
				</div>
			</div>
		</div>
	</section>
	<!-- Slider Part End -->

	<!-- imovel Tab -->
	<section id="imovel-tab">
