							<!--Filter List-->
							<!-- Tab Content -->
							<div class="row filter-list clearfix">
								<% for(prop of props) {%>
								<div class="column mix mix_all urbano col-md-3 col-sm-4 col-xs-12">
									<%- include('../includes/components/propiedade-grid') %>
								</div>
								<% } %>
							</div>
							<!-- End tab content -->
							<div class="row">
								<div class="col-md-12">
									<div class="float-right">
										<a role="button" href="/comprar" class="btn btn-default">Ver mais</a>
									</div>
								</div>
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
	</section>
	<!-- End imovel Tab -->

	<!-- Offer Part Start -->
